#==============================================================================
# Print Start G code
#==============================================================================
[gcode_macro PRINT_START]
gcode:   

    _SET_FLAP_ENABLED ENABLED=1
    lights_max
    M220 S100 # reset speed multiplier
    G90 ; test to see if this fixes out of range error
    # Parameters
    {% set bedtemp = params.BED|int %}
    {% set hotendtemp = params.HOTEND|int %}
    M104 S155 ; set extruder to 155deg
    M190 S{bedtemp} ; set & wait for bed temp
    G28 ; Home all axises
    _CLIENT_LINEAR_MOVE Z=5 F=1500
    #M190 S0 ; Turn off bed for mesh
    ; BED_MESH_CALIBRATE ADAPTIVE=0
    M190 S{bedtemp} ; set & wait for bed temp
    #NOZZLE_WIPE_LOCATION_READY
    M109 S{hotendtemp} 
    #NOZZLE_WIPE
    PRIME_LINE   
    